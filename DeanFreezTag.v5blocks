r1.Screen.clearScreen();\n  Controller1.Screen.setCursor(1, 1);\n  Controller1.Screen.print(printToController1_numberFormat(), static_cast<float>(Lives));\n  // stops the motors and waits for the freeze to be over\n  Drive1.stop();\n  Drive10.stop();\n  wait(3.0, seconds);\n}\n\n// User defined function\nvoid myblockfunction_Dual_Stick_Tank() {\n  // sets the motor speed based on the position of the joysticks, then moves the motors that speed\n  Drive1.setVelocity(Controller1.Axis3.position(), percent);\n  Drive10.setVelocity(Controller1.Axis2.position(), percent);\n  Drive1.spin(forward);\n  Drive10.spin(forward);\n}\n\n// User defined function\nvoid myblockfunction_Left_Stick_Drive() {\n  // uses just the left stick to drive the robot\n  Drive1.setVelocity((Controller1.Axis3.position() - (0.0 - Controller1.Axis4.position())), percent);\n  Drive10.setVelocity((Controller1.Axis3.position() + (0.0 - Controller1.Axis4.position())), percent);\n  Drive1.spin(forward);\n  Drive10.spin(forward);\n}\n\n// User defined function\nvoid myblockfunction_Split_Drive() {\n  // uses just the Right stick to drive the robot\n  Drive1.setVelocity((Controller1.Axis3.position() - (0.0 - Controller1.Axis1.position())), percent);\n  Drive10.setVelocity((Controller1.Axis3.position() + (0.0 - Controller1.Axis1.position())), percent);\n  Drive1.spin(forward);\n  Drive10.spin(forward);\n}\n\n// User defined function\nvoid myblockfunction_Right_Stick_Drive() {\n  // uses just the Right stick to drive the robot\n  Drive1.setVelocity((Controller1.Axis2.position() - (0.0 - Controller1.Axis1.position())), percent);\n  Drive10.setVelocity((Controller1.Axis2.position() + (0.0 - Controller1.Axis1.position())), percent);\n  Drive1.spin(forward);\n  Drive10.spin(forward);\n}\n\n// User defined function\nvoid myblockfunction_4WD_Split() {\n  Drive1.setVelocity((Controller1.Axis3.position() - (0.0 - Controller1.Axis1.position())), percent);\n  Drive10.setVelocity((Controller1.Axis3.position() + (0.0 - Controller1.Axis1.position())), percent);\n  Drive2.setVelocity((Controller1.Axis3.position() - (0.0 - Controller1.Axis1.position())), percent);\n  Drive9.setVelocity((Controller1.Axis3.position() + (0.0 - Controller1.Axis1.position())), percent);\n  Drive1.spin(forward);\n  Drive10.spin(forward);\n  Drive2.spin(forward);\n  Drive9.spin(forward);\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  // When the robot starts, Give it 3 lives and set the font on the screen\n  Lives = 4.0;\n  Brain.Screen.setFont(prop60);\n  // main loop displays the ammount of lives, then checks if the button is pressed\n  Brain.Screen.clearScreen();\n  Brain.Screen.setCursor(1, 1);\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Lives));\n  Controller1.Screen.clearScreen();\n  Controller1.Screen.setCursor(1, 1);\n  Controller1.Screen.print(printToController1_numberFormat(), static_cast<float>(Lives));\n  while ((!(Lives == 0.0))) {\n    if (BumperA.pressing() || BumperB.pressing()) {\n      // if either button is pressed, go to the freeze function and subtract the life by 1\n      Lives = Lives + -1.0;\n      myblockfunction_Freeze();\n    }\n    else {\n      // If neither button is pressed, allow the robot to drive\n      myblockfunction_Split_Drive();\n    }\n  wait(5, msec);\n  }\n  // once the robot is out of lives display the game over screen and stop the robot\n  Controller1.rumble(\"----\");\n  Brain.Screen.clearScreen();\n  Brain.Screen.setCursor(1, 1);\n  Brain.Screen.print(\"Game Over\");\n  return 0;\n}\n\n\nint main() {\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  whenStarted1();\n}","target":"Physical"}